<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#b7b7b7"
    android:orientation="vertical"
    tools:context="lecho.lib.hellocharts.samples.PieChartActivity"
    tools:ignore="MergeRootFrame" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Spinner
            android:layout_width="170dp"
            android:layout_height="51dp"
            android:id="@+id/allprojectspineer" />

        <Spinner
            android:layout_width="145dp"
            android:layout_height="46dp"
            android:id="@+id/thisweekspineer"
            android:layout_gravity="top|right"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />


    </RelativeLayout>




    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/relout"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#c7d7c7"
        android:layout_below="@+id/allprojectspineer">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TOP PROJECTS"
            android:textSize="20sp"
            android:id="@+id/textView4"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ALL PROJECTS"
            android:textSize="20sp"
            android:id="@+id/textView5"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"/>

    </RelativeLayout>

   <!-- <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
